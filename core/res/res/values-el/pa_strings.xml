<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- [CHAR LIMIT=NONE] Message shown in dialog on first boot after clean install for each .apk that is optimized. -->
    <string name="android_installing_apk">"Βελτιστοποίηση εφαρμογής
        <xliff:g id="number" example="123">%1$d</xliff:g> από
        <xliff:g id="number" example="123">%2$d</xliff:g>."</string>

    <!-- [CHAR LIMIT=40] Title of dialog that is shown when performing a system install. -->
    <string name="android_installing_title">"Το Android εγκαθιστά\u2026"</string>

    <!-- Power Menu (Global Actions) -->
    <!-- Label for item that takes a screenshot in phone options dialog. -->
    <string name="global_action_screenshot">"Στιγμιότυπο οθόνης"</string>
    <!-- label for item that reboots the phone in phone options dialog. -->
    <string name="global_action_reboot">"Επανεκκίνηση"</string>
    <!-- Reboot Progress Dialog. This is shown if the user chooses to reboot the phone. -->
    <string name="reboot_progress">"Γίνεται επανεκκίνηση\u2026"</string>
    <!-- Title of dialog to confirm rebooting. -->
    <string name="reboot_title">"Επανεκκίνηση"</string>
    <!-- Reboot Confirmation Dialog.  When the user chooses to reboot the device, there will
     be a confirmation dialog.  This is the message. -->
    <string name="reboot_confirm" product="tablet">"Το tablet σας θα κάνει επανεκκίνηση."</string>
    <string name="reboot_confirm" product="default">"Το τηλέφωνό σας θα κάνει επανεκκίνηση."</string>
    <!-- Sub-dialog labels. -->
    <string name="reboot">"Επανεκκίνηση"</string>
    <string name="reboot_recovery">"Λειτουργία ανάκτησης"</string>
    <string name="reboot_bootloader">"Bootloader"</string>

    <!-- Title of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <string name="permlab_accessThemeService">"πρόσβαση στην υπηρεσία θεμάτων"</string>
    <!-- Description of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <string name="permdesc_accessThemeService">"Επιτρέπει σε μια εφαρμογή να έχει πρόσβαση στην υπηρεσία θεμάτων. Κανονικά, δεν θα πρέπει ποτέ να χρειάζεται να ενεργοποιηθεί για κανονικές εφαρμογές."</string>

    <!-- Title of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <string name="permlab_readThemes">"ανάγνωση των πληροφοριών του θέματός σας"</string>
    <!-- Description of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <string name="permdesc_readThemesDesc">"Επιτρέπει στην εφαρμογή να κάνει ανάγνωση των θεμάτων σας και να προσδιορίσει ποιό θέμα έχετε εφαρμόσει."</string>

    <!-- Title of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <string name="permlab_writeThemes">"τροποποίηση των θεμάτων σας"</string>
    <!-- Description of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <string name="permdesc_writeThemesDesc">"Επιτρέπει στην εφαρμογή να εγκαθιστά νέα θέματα και να τροποποιεί το τρέχον θέμα."</string>

    <!-- Theme installation error notification -->
    <string name="theme_install_error_title">"Η εγκατάσταση του θέματος απέτυχε"</string>
    <string name="theme_install_error_message">"Το θέμα %s δεν μπόρεσε να εγκατασταθεί"</string>
    <string name="theme_reset_notification_title">"Επαναφορά θέματος"</string>
    <string name="theme_reset_notification_body">"Το προεπιλεγμένο θέμα συστήματος επαναφέρθηκε λόγω πολλαπλών σφαλμάτων εφαρμογών."</string>
</resources>
