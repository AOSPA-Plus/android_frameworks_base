<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- [CHAR LIMIT=NONE] Message shown in dialog on first boot after clean install for each .apk that is optimized. -->
    <string name="android_installing_apk">Оптимизация приложений
        <xliff:g id="number" example="123">%1$d</xliff:g> из
        <xliff:g id="number" example="123">%2$d</xliff:g>.</string>

    <!-- [CHAR LIMIT=40] Title of dialog that is shown when performing a system install. -->
    <string name="android_installing_title">Установка Android\u2026</string>

    <!-- Power Menu (Global Actions) -->
    <!-- Label for item that takes a screenshot in phone options dialog. -->
    <string name="global_action_screenshot">Снимок экрана</string>
    <!-- label for item that reboots the phone in phone options dialog. -->
    <string name="global_action_reboot">Перезагрузить</string>
    <!-- Reboot Progress Dialog. This is shown if the user chooses to reboot the phone. -->
    <string name="reboot_progress">Перезагрузка\u2026</string>
    <!-- Title of dialog to confirm rebooting. -->
    <string name="reboot_title">Перезагрузка</string>
    <!-- Reboot Confirmation Dialog.  When the user chooses to reboot the device, there will
     be a confirmation dialog.  This is the message. -->
    <string name="reboot_confirm" product="tablet">Ваш планшет будет перезагружен.</string>
    <string name="reboot_confirm" product="default">Ваш телефон будет перезагружен.</string>
    <!-- Sub-dialog labels. -->
    <string name="reboot">Перезагрузка</string>
    <string name="reboot_recovery">Режим восстановления</string>
    <string name="reboot_bootloader">Загрузчик</string>

    <!-- Title of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <string name="permlab_accessThemeService">доступ к службе тем</string>
    <!-- Description of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <string name="permdesc_accessThemeService">Позволяет приложению получить доступ к службе тем. Не должно требоваться для обычных приложений.</string>

    <!-- Title of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <string name="permlab_readThemes">cчитывать информормацию ваших тем</string>
    <!-- Description of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <string name="permdesc_readThemesDesc">Позволяет приложению считывать ваши темы и определять какая тема применена.</string>

    <!-- Title of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <string name="permlab_writeThemes">изменять ваши темы</string>
    <!-- Description of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <string name="permdesc_writeThemesDesc">Позволяет приложению добавлять новые темы и изменять тему, которая применена.</string>

    <!-- Theme installation error notification -->
    <string name="theme_install_error_title">Не удалось установить тему</string>
    <string name="theme_install_error_message">%s не удалось установить</string>
    <string name="theme_reset_notification_title">Восстановление темы</string>
    <string name="theme_reset_notification_body">Системная тема восстановлена из-за многократных падений приложения.</string>
</resources>
